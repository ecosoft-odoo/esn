<odoo>
    <!-- Tax Invoice / Delivery Order (A4) -->
    <record
        id="tax_invoice_delivery_order_a4_form_pdf_report"
        model="ir.actions.report"
    >
        <field name="name">Tax Invoice / Delivery Order (A4)</field>
        <field name="model">account.move</field>
        <field name="type">ir.actions.report</field>
        <field
            name="report_name"
        >esn_form.tax_invoice_delivery_order_a4_form_pdf</field>
        <field
            name="report_file"
        >esn_form.tax_invoice_delivery_order_a4_form_pdf</field>
        <field name="binding_model_id" ref="model_account_move" />
        <field name="binding_type">report</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="esn_form.invoice_paperformat" />
    </record>

    <!-- Invoice Preprint -->
    <record id="invoice_preprint_form_pdf_report" model="ir.actions.report">
        <field name="name">Invoice Preprint</field>
        <field name="model">account.move</field>
        <field name="type">ir.actions.report</field>
        <field name="report_name">esn_form.invoice_preprint_form_pdf</field>
        <field name="report_file">esn_form.invoice_preprint_form_pdf</field>
        <field name="binding_model_id" ref="model_account_move" />
        <field name="binding_type">report</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="esn_form.invoice_preprint_paperformat" />
    </record>

    <!-- Invoice Preprint (No Serial) -->
    <record id="invoice_preprint_no_serial_form_pdf_report" model="ir.actions.report">
        <field name="name">Invoice Preprint (No Serial)</field>
        <field name="model">account.move</field>
        <field name="type">ir.actions.report</field>
        <field name="report_name">esn_form.invoice_preprint_no_serial_form_pdf</field>
        <field name="report_file">esn_form.invoice_preprint_no_serial_form_pdf</field>
        <field name="binding_model_id" ref="model_account_move" />
        <field name="binding_type">report</field>
        <field name="report_type">qweb-pdf</field>
        <field name="paperformat_id" ref="esn_form.invoice_preprint_paperformat" />
    </record>
</odoo>
